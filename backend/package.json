{
  "name": "exploratour-backend",
  "author": "Wellington Lima",
  "version": "1.0.0",
  "description": "Backend Exploratour",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "test": "vitest -w",
    "services:up": "docker compose -f src/compose.yaml up -d",
    "services:down": "docker compose -f src/compose.yaml down",
    "migration:create": "node-pg-migrate -m src/infra/migrations create -j ts"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "node-pg-migrate": "^8.0.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
